{
  "name": "josedromero",
  "homepage": "https://josedromero.com",
  "version": "0.1.0",
  "main": "./src/index.js",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-pdf": "^5.0.0",
    "react-router-dom": "^5.2.0",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "4.0.0",
    "swiper": "^6.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "sitemap": "babel-node ./sitemap-builder.js",
    "css-build": "node-sass --omit-source-map-url ./src/sass/mystyles.scss ./src/main.css",
    "css-watch": "npm run css-build -- --watch",
    "css-start": "npm run css-watch",
    "css-nanofy": "npx postcss ./src/main.css > ./src/index.css",
    "build-production": "npm run css-build && npm run css-nanofy && npm run sitemap && npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bulma": "^0.9.1",
    "cssnano": "^5.0.16",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "node-sass": "^4.14.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0"
  }
}
